{
  "name": "1990towin",
  "scripts": {
    "install-if-needed": "cmp -s package.json ._package.json || (npm install && cp package.json ._package.json)",
    "start": "node ./server/index.js",
    "build:client": "npm run install-if-needed && webpack -p",
    "dev:client": "webpack --watch --mode=development",
    "dev:server": "NODE_ENV=dev nodemon -w ./ -e css,js,json,html ./server/index.js",
    "dev": "npm run install-if-needed && npm-run-all -p dev:*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "panel": "^2.6.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
